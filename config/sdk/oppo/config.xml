<?xml version="1.0" encoding="UTF-8"?>
<config>
	<operations>
		<operation step="1" type="merge" from="manifest" to="AndroidManifest.xml" />
		<operation step="2" type="copy" from="libs" to="lib" />
		<operation step="3" type="copy" from="assets" to="assets" />
		<operation step="4" type="copy" from="res" to="res" />
	</operations>
	<plugins>
		<plugin name="com.ubsdk.oppo.plugin.OPPOUserPlugin" type="1" />
		<plugin name="com.ubsdk.oppo.plugin.OPPOPayPlugin" type="2" />
		<plugin name="com.ubsdk.oppo.plugin.OPPOSettingPlugin" type="3" />
	</plugins>
	<applications>
		<application name="com.ubsdk.oppo.plugin.OPPOApplication" />
	</applications>
</config>