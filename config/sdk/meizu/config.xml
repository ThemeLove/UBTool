		<operation step="2" type="copy"  from="libs" to="lib"/>
		<operation step="3" type="copy"  from="assets" to="assets"/>
		<operation step="4" type="copy"  from="res" to="res"/>
	</operations>
	<plugins>
		<plugin name="com.ubsdk.meizu.plugin.MeiZuUserPlugin" type="1" />
		<plugin name="com.ubsdk.meizu.plugin.MeiZuPayPlugin"  type="2"/>
		<plugin name="com.ubsdk.meizu.plugin.MeiZuSettingPlugin"  type="3"/>
	</plugins>
	<applications>
		<application name="com.ubsdk.meizu.plugin.MeiZuApplication"/>
	</applications>
</config>